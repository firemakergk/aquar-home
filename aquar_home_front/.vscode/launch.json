{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "chrome",
        "request": "launch",
        "name": "vuejs: chrome",
        "url": "http://localhost:9528",
        "webRoot": "${workspaceFolder}/src",
        "breakOnLoad": true,
        "sourceMapPathOverrides": {
          // "webpack:///./src/*": "${webRoot}/*",
          "webpack:///src/*": "${webRoot}/*"
        },
      }
    ]
  }